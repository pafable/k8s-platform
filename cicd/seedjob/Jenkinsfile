pipeline {
    agent {
      label 'jenkins-jenkins-agent'
    }

    tools {
      terraform 'tf-1-9-2'
    }


    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('tf check') {
            steps {
                sh 'terraform version'
            }
        }

        stage('sleep') {
            steps {
                sh 'sleep 15'
            }
        }
    }
}